const FIZZ_NUMBER = 3;
const BUZZ_NUMBER = 5;
const MAX_COUNT = 100;
const FIZZ_WORD = "Fizz";
const BUZZ_WORD = "Buzz";

function fizzBuzz(maxCount = MAX_COUNT, fizzNum = FIZZ_NUMBER, buzzNum = BUZZ_NUMBER, fizzWord = FIZZ_WORD, buzzWord = BUZZ_WORD) {
    for (let i = 1; i <= maxCount; i++) {
        if (i % fizzNum === 0 && i % buzzNum === 0) {
            console.log(fizzWord + buzzWord);
        } else if (i % fizzNum === 0) {
            console.log(fizzWord);
        } else if (i % buzzNum === 0) {
            console.log(buzzWord);
        } else {
            console.log(i);
        }
    }
}

// Call with default values (uses constants)
fizzBuzz();

// Or with custom values
fizzBuzz(50, 3, 7, "Foo", "Bar");

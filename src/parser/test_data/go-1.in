package main

import "fmt"

const (
	FIZZ_NUMBER = 3
	BUZZ_NUMBER = 5
	MAX_COUNT   = 100
	FIZZ_WORD   = "Fizz"
	BUZZ_WORD   = "Buzz"
)

func main() {
	for i := 1; i <= MAX_COUNT; i++ {
		switch {
		case i%FIZZ_NUMBER == 0 && i%BUZZ_NUMBER == 0:
			fmt.Println(FIZZ_WORD + BUZZ_WORD)
		case i%FIZZ_NUMBER == 0:
			fmt.Println(FIZZ_WORD)
		case i%BUZZ_NUMBER == 0:
			fmt.Println(BUZZ_WORD)
		default:
			fmt.Println(i)
		}
	}
}
